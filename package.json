{
  "name": "regexforge",
  "displayName": "RegexForge",
  "description": "Use ChatGPT to create regex expressions",
  "repository": {
    "type": "git",
    "url": "https://github.com/nectariosouzou/regexforge"
  },
  "publisher": "NectariosOuzounidisRegexForge",
  "icon": "assets/logo.png",
  "version": "1.0.1",
  "engines": {
    "vscode": "^1.81.x"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartup"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "keybindings": [
      {
        "command": "regexforge.create",
        "key": "ctrl+alt+r",
        "mac": "cmd+ctrl+r"
      }
    ],
    "commands": [
      {
        "command": "regexforge.create",
        "title": "Create A Regex"
      }
    ]
  },
  "scripts": {
    "deploy": "vsce publish",
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/minimatch": "^5.1.2",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/node-fetch": "^2.6.4",
    "@types/sinon": "^10.0.16",
    "@types/vscode": "^1.81.x",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "@vscode/test-electron": "^2.3.4",
    "@vscode/vsce": "^2.20.1",
    "eslint": "^8.47.0",
    "mocha": "^10.2.0",
    "sinon": "^15.2.0",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "glob": "^10.3.3",
    "node-fetch": "^2.7.0"
  }
}
